<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <ControlTheme x:Key="{x:Type Panel}" TargetType="Panel" BasedOn="{StaticResource {x:Type Panel}}">
        <Style Selector="^.TextBold /template/ TextBlock">
            <Setter Property="FontWeight" Value="DemiBold"></Setter>
        </Style>
    </ControlTheme>

</ResourceDictionary>
